		"@babel/preset-env": "^7.13.15",
		"babel-loader": "^8.2.2",
		"css-loader": "^5.2.4",
		"eslint": "^7.24.0",
		"eslint-config-xbsoftware": "git+https://bitbucket.org/xbsltd/eslint.git#v4.0.0-alpha.3",
		"eslint-plugin-import": "^2.22.1",
		"file-loader": "^6.2.0",
		"husky": "^6.0.0",
		"less": "^4.1.1",
		"less-loader": "^8.1.1",
		"lint-staged": "^10.5.4",
