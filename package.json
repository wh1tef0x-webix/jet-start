{
	"name": "webix-jet-app",
	"version": "1.3.0",
	"description": "Webix Jet App",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"main": "sources/app.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint sources/",
		"build": "webpack --env.production true",
